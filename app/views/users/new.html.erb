

<h1>This is my new form </h1>

<%= render 'errors'%>

<%= form_for @user do | f | %>
<%= f.label :name %>
<%= f.text_field :name %>
<br>
<%= f.label :email %>
<%= f.text_field :email %>
<br>
<%= f.label :password %>
<%= f.password_field :password %>
<br>
<%= f.label :password_confirmation %>
<br>
<%= f.password_field :password_confirmation %>
<br>
<%= f.submit 'create user'%>
<% end %>
<%= button_to "Log out", logout_path, method: :delete %>